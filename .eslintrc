{
  "parser": 'babel-eslint',
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": 0,
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-uses-react": 1,
    "react/prefer-stateless-function": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
